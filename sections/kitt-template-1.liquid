<div class="page-width feature-row" style="width: 100%;">
  {% capture image_layout %}
    <div class="feature-row__item" style="width: 50%; {{image_order}}">
      {% if section.settings.image != blank %}
        {% capture img_id %}FeatureRowImage-{{ section.id }}{% endcapture %}
        {% capture wrapper_id %}FeatureRowImageWrapper-{{ section.id }}{% endcapture %}
        {%- assign img_url = section.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
        {% include 'image-style', image: section.settings.image, height: 545, wrapper_id: wrapper_id, img_id: img_id %}
        <div id="{{ wrapper_id }}" class="feature-row__image-wrapper js">
          <div style="width: 100%;" data-image-loading-animation>
            <img id="{{ img_id }}"
                 class="lazyload"
                 data-src="{{ img_url }}"
                 data-aspectratio="{{ section.settings.image.aspect_ratio }}"
                 data-sizes="auto"
                 alt="{{ section.settings.image.alt | escape }}"
                 style="width: 100% !important; max-width: 100% !important; height: 386px;"
                 >
          </div>
        </div>

        <noscript>
          {{ section.settings.image | img_url: '600x600', scale: 2 | img_tag: section.settings.image.alt, 'feature-row__image' }}
        </noscript>
      {% else %}
        {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
      {% endif %}
    </div>
  {% endcapture %}

  <div class="feature-row" style="display: flex; align-items: flex-start;">
    
    {% if section.settings.layout == 'left' %}
      {%- assign text_order = 'order: 2;'-%}
      {%- assign include_image_order = 'order: 1;'-%}
      {{ image_layout }}
    {% endif %}
    
    {% if section.settings.layout == 'right' %}
      {%- assign text_order = 'width: 80%; order: 1;'-%}
      {%- assign include_image_order = 'width: 20%; order: 2; padding-right: 20;'-%}
    {% endif %}
    
    
    <div class="feature-row  feature-row__text--{{ section.settings.layout }}" style="width: 50%; align-items: flex-start;">
      <div class="" style="display: flex !important; justify-content: space-between !important; flex-direction: column !important;{{include_image_order}}">
        {%- assign img_url1 = section.settings.image1 | img_url: '1x1' | replace: '_1x1.', '_1024x1024@2x.' -%}
        <div class="" style="border: 1px solid black; height: 120px; width: 150px; text-align: center; background-image: url('https:{{ img_url1 }}'); background-repeat: no-repeat; background-position: center; background-size: 105% 100%;">
<!--           <h4>INCLUDED PRODUCT IMAGE</h4> -->
        </div>
        {%- assign img_url2 = section.settings.image2 | img_url: '1x1' | replace: '_1x1.', '_1024x1024@2x.' -%}
        <div class="" style="border: 1px solid black; height: 120px; width: 150px; margin-top: 10px; margin-bottom: 15px; text-align: center; background-image: url('https:{{ img_url2 }}'); background-repeat: no-repeat; background-position: center; background-size: 105% 100%;">
<!--           <h4>INCLUDED PRODUCT IMAGE</h4> -->
        </div>
        {%- assign img_url3 = section.settings.image3 | img_url: '1x1' | replace: '_1x1.', '_1024x1024@2x.' -%}
        <div class="" style="border: 1px solid black; height: 120px; width: 150px; text-align: center; background-image: url('https:{{ img_url3 }}'); background-repeat: no-repeat; background-position: center; background-size: 105% 100%;">
<!--           <h4>INCLUDED PRODUCT IMAGE</h4> -->
        </div>
      </div>
      <div style="padding-left: 20px; padding-right: 20px; {{text_order}}">
        {% if section.settings.title != blank %}
        <h2 class="h3">{{ section.settings.title | escape }}</h2>
        {% endif %}
        {% if section.settings.text != blank %}
        <div class="kit_text rte rte-setting featured-row__subtext">
          {{ section.settings.text }}
        </div>
        <div class="page-width feature-row">
          <div class="kit-template-icon_container text-center">
            {% include 'icon-trophy' %}
            {{section.settings.benefit-1}}
          </div>
          <div class="kit-template-icon_container text-center">
            {% include 'icon-dumbbell' %}
            {{section.settings.benefit-2}}
          </div>
          <div class="kit-template-icon_container text-center">
            {% include 'icon-shaker' %}
            {{section.settings.benefit-3}}
          </div>
        </div>
        <div style="margin-top: 20px;">
        	<h1 class="h1">$ {{section.settings.price}}</h1>
        </div>
        {% capture "form_classes" -%}
          product-form product-form-{{ section.id }}
          {%- if section.settings.enable_payment_button and product.has_only_default_variant %} product-form--payment-button-no-variants {%- endif -%}
          {%- if current_variant.available == false %} product-form--variant-sold-out {%- endif -%}
        {%- endcapture %}

<!--         {% form 'product', product, class:form_classes, novalidate: 'novalidate', data-product-form: '' %}
          {% if section.settings.enable_payment_button %}

            <div class="product_payment_btn">
              {{ form | payment_button }}
            </div>
          {% endif %}
        {% endform %} -->
        {% endif %}
        {% if section.settings.button_label != blank and section.settings.button_link != blank %}
        {%- capture ariaLabel -%}
        {{- section.settings.button_label -}}
        {%- if section.settings.button_link.type == 'frontpage_link' -%}
        : {{ 'homepage.general.title' | t -}}
        {%- elsif section.settings.button_link.type == 'catalog_link' -%}
        : {{ 'collections.catalog.title' | t -}}
        {%- elsif section.settings.button_link.object.title -%}
        : {{ section.settings.button_link.object.title -}}
        {%- endif -%}
        {%- endcapture -%}
<!-- 
        <a href="{{ section.settings.button_link }}"
           class="btn"
           aria-label="{{ ariaLabel }}">
          {{- section.settings.button_label | escape -}}
        </a> -->
        {% endif %}
        
        {% if section.settings.shop_button_label != '' and section.settings.shop_button_link != '' %}
          <a href="{{ section.settings.shop_button_link }}"
            class="btn btn--secondary btn--small shop_now_kit"
            aria-label="{{ shop_button_label | escape }}">
            {{- section.settings.shop_button_label | escape -}}
          </a>
        {% endif %}
      </div>
    </div>
    {% if section.settings.layout == 'right' %}
      {{image_layout}}
    {% endif %}
  </div>
</div>

{% unless product == empty %}
  <script type="application/json" id="ProductJson-{{ section.id }}">
    {{ product | json }}
  </script>
  <script type="application/json" id="ModelJson-{{ section.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{% endunless %}


